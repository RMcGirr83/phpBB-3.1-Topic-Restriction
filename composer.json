{
    "name": "rmcgirr83/topicrestriction",
    "type": "phpbb-extension",
    "description": "Adds a new forum permission which can send new topics to moderation.  Once enabled you must set which forums, under the Misc tab of the forum, you want to restrict viewing of the topics.",
    "version": "1.0.0",
    "time": "2015-12-06",
    "keywords": [
        "phpbb",
        "extension",
        "profile",
        "communication"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Rich McGirr",
            "homepage": "https://www.phpbb.com/customise/db/author/rmcgirr83/contributions",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Topic Restriction",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0,<3.3.*@dev"
        }
    }
}